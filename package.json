{
  "name": "bentes6",
  "version": "2.0.0",
  "description": "Node.js, Typescript Babel 7 and ES6 setup",
  "main": "server.js",
  "scripts": {
    "start": "nodemon",
    "start:dev": "NODE_ENV=dev nodemon",
    "start:prod": "NODE_ENV=prod nodemon",
    "build": "babel src --out-dir dist --extensions \".ts, .tsx\" --source-maps inline",
    "serve": "node dist/server.js",
    "test:newman": "node src/test/newman.js"
  },
  "keywords": [
    "Babel7",
    "nodejs",
    "Typescript",
    "express.js"
  ],
  "author": "Ajantha Bandara",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "newman": "^5.3.2",
    "nodemon": "^2.0.20",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/joi": "^14.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "joi": "^14.0.4"
  }
}
