{
  "name": "bentes6",
  "version": "1.1.0",
  "description": "Node.js, Typescript Bable 7 and ES6 setup",
  "main": "server.js",
  "scripts": {
    "start": "nodemon",
    "start:dev": "NODE_ENV=dev nodemon",
    "start:prod": "NODE_ENV=prod nodemon",
    "build": "babel src --out-dir dist --extensions \".ts, .tsx\" --source-maps inline",
    "serve": "node dist/server.js",
    "test:newman": "node src/test/newman.js"
  },
  "keywords": [
    "Bable7",
    "nodejs",
    "Typescript",
    "expressjs"
  ],
  "author": "Ajantha Bandara",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "newman": "^4.5.7",
    "nodemon": "^1.19.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.4",
    "body-parser": "^1.18.3",
    "core-js": "^3.6.4",
    "exceljs": "^3.6.1",
    "express": "^4.16.3",
    "joi": "^14.0.4",
    "regenerator-runtime": "^0.13.3"
  }
}
